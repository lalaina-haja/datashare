# Application (make sure ports are available)
API_PORT=8080
WEB_PORT=4200
APP_HOST=http://localhost
APP_NAME=DataShare

# Security (change JWT_SECRET value to a real secret)
JWT_SECRET=your-256-bit-secret-key-here-change-in-prod-32-chars-min
JWT_EXPIRATION=3600000

# Database 
DB_NAME=database_name
DB_USER=database_user
DB_PASSWORD=datashare_password
DB_HOST=localhost
DB_PORT=5432

# AWS S3
AWS_REGION=aws_localstack_region
AWS_S3_BUCKET=bucket_name

# Testing
TEST_EMAIL=test@example.com
TEST_PASS=Passw0rd!
# Cypress overrides (can be set in CI or locally to override defaults)
CYPRESS_TEST_EMAIL=${TEST_EMAIL}
CYPRESS_TEST_PASS=${TEST_PASS}