{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "rootDir": "./src",
        "outDir": "./out-tsc/e2e",
        "types": [
            "cypress",
            "node"
        ],
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true
    },
    "include": [
        "src/test/e2e/**/*.ts",
    ]
}