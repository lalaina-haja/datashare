{
  "name": "datashare-web",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "set-env": "../scripts/sync-env-web.sh",
    "start": "npm run set-env && ng serve --port $WEB_PORT",
    "build": "npm run set-env && ng build ",
    "watch": "npm run set-env && ng build --watch --configuration development",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --config=config/vitest.unit.config.ts",
    "test:unit:ui": "vitest --config=config/vitest.unit.config.ts --ui",
    "test:unit:coverage": "vitest --config=config/vitest.unit.config.ts --coverage",
    "test:unit:run": "vitest run --config=config/vitest.unit.config.ts",
    "test:integration": "vitest --config=config/vitest.integration.config.ts",
    "test:integration:ui": "vitest --config=config/vitest.integration.config.ts --ui",
    "test:integration:coverage": "vitest --config=config/vitest.integration.config.ts --coverage",
    "test:integration:run": "vitest run --config=config/vitest.integration.config.ts",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run",
    "test:e2e:headless": "cypress run --headless",
    "test:e2e:chrome": "cypress run --browser chrome",
    "test:e2e:firefox": "cypress run --browser firefox",
    "test:all": "npm run test:unit:run && npm run test:integration:run && npm run test:e2e:run",
    "test:ci": "npm run test:unit:run && npm run test:integration:run && npm run test:e2e:headless",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.{ts,html,scss,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,css,json}\"",
    "typedoc": "typedoc src/index.ts src/app/ --out ../../docs/typedoc",
    "docs": "npm run typedoc"
  },
  "private": true,
  "packageManager": "npm@11.7.0",
  "dependencies": {
    "@angular/animations": "^21.1.2",
    "@angular/cdk": "^21.1.2",
    "@angular/common": "^21.1.2",
    "@angular/compiler": "^21.1.2",
    "@angular/core": "^21.1.2",
    "@angular/forms": "^21.1.2",
    "@angular/material": "^21.1.2",
    "@angular/platform-browser": "^21.1.2",
    "@angular/platform-browser-dynamic": "^21.1.2",
    "@angular/router": "^21.1.2",
    "rxjs": "^7.8.2",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.2.3",
    "@analogjs/vitest-angular": "^2.2.3",
    "@angular-eslint/builder": "^21.2.0",
    "@angular-eslint/eslint-plugin": "^21.2.0",
    "@angular-eslint/eslint-plugin-template": "^21.2.0",
    "@angular-eslint/schematics": "^21.2.0",
    "@angular-eslint/template-parser": "^21.2.0",
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.2",
    "@badeball/cypress-cucumber-preprocessor": "^24.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.8",
    "@eslint/js": "^9.39.2",
    "@testing-library/angular": "^19.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.1.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "angular-eslint": "^21.2.0",
    "cypress": "^15.9.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.1",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "src/test/e2e/steps/**/*.ts",
    "messages": {
      "enabled": true
    }
  }
}
